#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@moonwall+cli@4.4.4_@types+node@20.10.3_typescript@5.3.2/node_modules/@moonwall/cli/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@moonwall+cli@4.4.4_@types+node@20.10.3_typescript@5.3.2/node_modules/@moonwall/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@moonwall+cli@4.4.4_@types+node@20.10.3_typescript@5.3.2/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@moonwall+cli@4.4.4_@types+node@20.10.3_typescript@5.3.2/node_modules/@moonwall/cli/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@moonwall+cli@4.4.4_@types+node@20.10.3_typescript@5.3.2/node_modules/@moonwall/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@moonwall+cli@4.4.4_@types+node@20.10.3_typescript@5.3.2/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --no-warnings "$basedir/../@moonwall/cli/moonwall.mjs" "$@"
else
  exec node  --no-warnings "$basedir/../@moonwall/cli/moonwall.mjs" "$@"
fi
