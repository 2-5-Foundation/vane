{
  "name": "napi-maybe-compressed-blob",
  "version": "0.0.11",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "napi-maybe-compressed-blob",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.11.4",
    "mocha": "9.1.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "mocha index.test.js"
  },
  "optionalDependencies": {
    "napi-maybe-compressed-blob-darwin-arm64": "0.0.11",
    "napi-maybe-compressed-blob-linux-arm64-gnu": "0.0.11",
    "napi-maybe-compressed-blob-darwin-x64": "0.0.11",
    "napi-maybe-compressed-blob-linux-x64-gnu": "0.0.11"
  }
