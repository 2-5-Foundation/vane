{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/wasm/issues",
  "description": "Utilities used exclusively inside Wasm decoding",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/polkadot-js/wasm/tree/master/packages/wasm-util#readme",
  "license": "Apache-2.0",
  "name": "@polkadot/wasm-util",
  "repository": {
    "directory": "packages/wasm-util",
    "type": "git",
    "url": "https://github.com/polkadot-js/wasm.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./cjs/detectPackage.js"
  ],
  "type": "module",
  "version": "7.3.2",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./base64": {
      "types": "./base64.d.ts",
      "module": "./base64.js",
      "require": "./cjs/base64.js",
      "default": "./base64.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "module": "./bundle.js",
      "require": "./cjs/bundle.js",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "module": "./detectOther.js",
      "require": "./cjs/detectOther.js",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "module": "./detectPackage.js",
      "require": "./cjs/detectPackage.js",
      "default": "./detectPackage.js"
    },
    "./fflate": {
      "types": "./fflate.d.ts",
      "module": "./fflate.js",
      "require": "./cjs/fflate.js",
      "default": "./fflate.js"
    },
    "./package.json": {
      "require": "./cjs/package.json",
      "default": "./package.json"
    },
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    }
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@polkadot/util": "*"
  }
}
