#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules/@acala-network/chopsticks/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules/@acala-network/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules/@acala-network/chopsticks/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules/@acala-network/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules:/Users/mrisholukamba/Desktop/subzero/parachain/vane/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules/@acala-network/chopsticks/chopsticks.cjs" "$@"
else
  exec node  "$basedir/../../../../../../@acala-network+chopsticks@0.9.3_debug@4.3.4/node_modules/@acala-network/chopsticks/chopsticks.cjs" "$@"
fi
