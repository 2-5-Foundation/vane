{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/wasm/issues",
  "description": "Init handlers for wasm-crypto",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/polkadot-js/wasm/tree/master/packages/wasm-crypto-init#readme",
  "license": "Apache-2.0",
  "name": "@polkadot/wasm-crypto-init",
  "repository": {
    "directory": "packages/wasm-crypto-init",
    "type": "git",
    "url": "https://github.com/polkadot-js/wasm.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./cjs/detectPackage.js"
  ],
  "type": "module",
  "version": "7.3.2",
  "main": "./cjs/wasm.js",
  "module": "./wasm.js",
  "browser": "./cjs/wasm.js",
  "react-native": "./cjs/asm.js",
  "types": "./wasm.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./wasm.d.ts",
      "react-native": {
        "module": "./asm.js",
        "require": "./cjs/asm.js",
        "default": "./asm.js"
      },
      "browser": {
        "module": "./wasm.js",
        "require": "./cjs/wasm.js",
        "default": "./wasm.js"
      },
      "node": {
        "module": "./wasm.js",
        "require": "./cjs/wasm.js",
        "default": "./wasm.js"
      }
    },
    "./asm": {
      "types": "./asm.d.ts",
      "module": "./asm.js",
      "require": "./cjs/asm.js",
      "default": "./asm.js"
    },
    "./both": {
      "types": "./both.d.ts",
      "module": "./both.js",
      "require": "./cjs/both.js",
      "default": "./both.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "module": "./bundle.js",
      "require": "./cjs/bundle.js",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "module": "./detectOther.js",
      "require": "./cjs/detectOther.js",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "module": "./detectPackage.js",
      "require": "./cjs/detectPackage.js",
      "default": "./detectPackage.js"
    },
    "./none": {
      "types": "./none.d.ts",
      "module": "./none.js",
      "require": "./cjs/none.js",
      "default": "./none.js"
    },
    "./package.json": {
      "require": "./cjs/package.json",
      "default": "./package.json"
    },
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "module": "./types.js",
      "require": "./cjs/types.js",
      "default": "./types.js"
    },
    "./wasm": {
      "types": "./wasm.d.ts",
      "module": "./wasm.js",
      "require": "./cjs/wasm.js",
      "default": "./wasm.js"
    }
  },
  "dependencies": {
    "@polkadot/wasm-bridge": "7.3.2",
    "@polkadot/wasm-crypto-asmjs": "7.3.2",
    "@polkadot/wasm-crypto-wasm": "7.3.2",
    "@polkadot/wasm-util": "7.3.2",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@polkadot/util": "*",
    "@polkadot/x-randomvalues": "*"
  }
}
